#ifndef API_INC_API_UART_H_
#define API_INC_API_UART_H_
#endif /* API_INC_API_UART_H_ */

typedef uint8_t tick_t;
typedef bool bool_t;

typedef struct{
	tick_t temperature;
	tick_t time;
}recipe_t;


void UART_Init(void);
void UART_recipeTemperature(receipe_t*temperature);
void UART_recipeTime(receipe_t*time);
void UART_sed(void);
