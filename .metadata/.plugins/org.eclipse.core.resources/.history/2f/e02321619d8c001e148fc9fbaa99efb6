#include "API_delay.h"

	void delayInit (delay_t*delay){
		delay->running=false;
		delay->duration=0;
	}
	// Se inicializa con condiciones iniciales.

	void delayWrit (delay_t*delay, tick_t duration){
		delay->duration=duration;
	}
