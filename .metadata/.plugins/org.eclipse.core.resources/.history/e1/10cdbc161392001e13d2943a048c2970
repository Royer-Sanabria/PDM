#include "main.h"
#include "API_UART.h"


static void MX_USART2_UART_Init(void);


void UART_receiveTemperature(recipe_t*temperature){
	UART_HandleTypeDef huart2;
	MX_USART2_UART_Init();
	 char p[]={"hola"};
	HAL_UART_Transmit(&huart2,(uint8_t *)p, 4, HAL_MAX_DELAY);
}
