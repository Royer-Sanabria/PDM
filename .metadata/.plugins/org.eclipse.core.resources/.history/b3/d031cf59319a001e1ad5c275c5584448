void MEF(){
	Estado FlagMEF=Inicio;
	switch (FlagMEF)

	case Inicio:
char mensajeAlturaSeteo[]={"Indique la altura de seteo: "};
HAL_UART_Transmit(&huart2, (uint8_t *)mensajeAltura, strlen(mensajeAlturaSeteo), 100);
FlagMEF=Altura_Seteo;
break

case Altura_Seteo:

break;


	default:
	FlagMEF=Altura_Seteo;
}
