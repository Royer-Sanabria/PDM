#include "main.h"
#include "BMP280_Funtion.h"
#include "API_Delay.h"
#include "API_UART.h"
#include "MEF.h"
#include <stdio.h>
#include <string.h>
#include <stdlib.h>

static enum Estado FlagMEF=Inicio;


void MEF(){

	switch (FlagMEF){

	case Inicio:
char mensajeAlturaSeteo[]={"Indique la altura de seteo: "};
UART_TrasmitMessage(mensajeAlturaSeteo);
FlagMEF=Altura_Seteo;
break;

	default:
	FlagMEF=Altura_Seteo;
	}
}
