#include "main.h"
#include "BMP280_Funtion.h"
#include "API_Delay.h"
#include "API_UART.h"
#include "MEF.h"

enum Estado {
    Inicio,
	Altura_Seteo,
	Alerta_Abajo,
	Altura_Alcanzada,
	Alerta_Arriba
};


void MEF(){
	Estado FlagMEF=Inicio;
	switch (FlagMEF)

	case Inicio:
char mensajeAlturaSeteo[]={"Indique la altura de seteo: "};
HAL_UART_Transmit(&huart2, (uint8_t *)mensajeAltura, strlen(mensajeAlturaSeteo), 100);
FlagMEF=Altura_Seteo;
break;

case Altura_Seteo:

break;


	default:
	FlagMEF=Altura_Seteo;
}
